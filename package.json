{
  "name": "blockplot",
  "version": "0.0.1",
  "subdomain": "blockplot",
  "dependencies": {
    "gravatar": "~1.0.6",
    "voxel-level": "1.0.0",
    "voxel-bundle": "0.0.0",
    "minecraft-blockinfo": "~0.2.0",
    "voxel-hello-world": "~0.5.0",
    "voxel-fly": "~0.1.0",
    "mca2js": "~0.2.0",
    "hoodie": "0.1.28",
    "workerstream": "~1.0.0"
  },
  "scripts": {
    "start": "beefy main.js:index-bundle.js world.js:world-bundle.js convert-worker.js:convert-worker-bundle.js world-worker.js:world-worker-bundle.js --debug=false -- --fast  --noparse=three",
    "build": "browserify main.js -o index-bundle.js && browserify world.js -o world-bundle.js  && browserify convert-worker.js -o convert-worker-bundle.js && browserify world-worker.js -o world-worker-bundle.js"
  },
  "engines": {
    "node": "0.8.x"
  },
  "devDependencies": {
    "beefy": "~0.3.0",
    "browserify": "~2.14.2"
  }
}
